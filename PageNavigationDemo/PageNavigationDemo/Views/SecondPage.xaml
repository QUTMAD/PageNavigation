        <StackLayout>
            <Label x:Name="FintSize" Text="{Binding NavigationText}" FontSize="{Binding NavigationSize}" TextColor="{Binding TextColour}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>